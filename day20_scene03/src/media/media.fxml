<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="323.0" prefWidth="521.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="media.Controller">
   <children>
      <ListView fx:id="listView" layoutX="276.0" layoutY="48.0" prefHeight="200.0" prefWidth="200.0" />
      <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" layoutX="35.0" layoutY="48.0" />
      <Button fx:id="btnPlay" layoutX="35.0" layoutY="279.0" mnemonicParsing="false" onAction="#playFunc" text="재생" />
      <Button fx:id="btnPause" layoutX="103.0" layoutY="279.0" mnemonicParsing="false" onAction="#pauseFunc" text="일시정지" />
      <Button fx:id="btnStop" layoutX="195.0" layoutY="279.0" mnemonicParsing="false" onAction="#stopFunc" text="정지" />
   </children>
</AnchorPane>
